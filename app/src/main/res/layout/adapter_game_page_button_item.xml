<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <TextView
        android:layout_gravity="center_horizontal"
        android:layout_margin="@dimen/spacing_16"
        android:gravity="center"
        android:background="@drawable/action_button_new99_trans10"
        android:textColor="#ffffff"
        android:id="@+id/adapter_game_page_button_jump_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:layout_marginTop="16dp"
        android:layout_gravity="center_horizontal"
        android:layout_margin="@dimen/spacing_16"
        android:gravity="center"
        android:background="@drawable/action_button_new99_trans13"
        android:textColor="#ffffff"
        android:id="@+id/adapter_game_page_button_jump_button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:layout_marginTop="16dp"
        android:layout_gravity="center_horizontal"
        android:layout_margin="@dimen/spacing_16"
        android:gravity="center"
        android:background="@drawable/action_button_new99_trans12"
        android:textColor="#ffffff"
        android:id="@+id/adapter_game_page_button_jump_button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
</LinearLayout>